<Page x:Class="Demo1.Pages.Admin.DsPheDuyet"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Demo1.Pages.Admin"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MemberListPage">
    <Grid Margin="10">
        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
            <StackPanel Margin="20">
                <TextBlock Text="DANH SÁCH PHÊ DUYỆT" Foreground="{StaticResource TitlePrimaryColor}"  FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="24" Margin="0,0 ,0,20"/>
                <local:DsPheDuyetControl DataContext="{Binding DanhsachPheDuyet}" />
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Page>
